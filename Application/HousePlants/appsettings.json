{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostgresConnection": "Host=localhost;Database=HousePlants;Username=postgres;Password=postgres"
  },
  "Serilog": {
    "Using": [ "Serilog.Exceptions", "Serilog", "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "restrictedToMinimumLevel": "Error"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails", "Environment" ],
    "Properties": {
      "Environment": "Ich_habe_nicht_ideen!"
    }
  },
  "ENV_SQL_HOST": "localhost",
  "ENV_SQL_USERNAME": "postgres",
  "ENV_SQL_PASSWORD": "postgres" 
}